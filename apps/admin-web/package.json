{
  "name": "@evc/app-admin",
  "version": "1.0.0",
  "private": true,
  "description": "EV Charging Admin Panel - Next.js 15 + React 19",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:fast": "next dev --turbopack --experimental-https",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:profile": "next build --profile",
    "build:debug": "next build --debug",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "preview": "next build && next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "biome check --write src/",
    "lint:fix": "biome check --fix src/",
    "format": "biome format --write src/",
    "type-check": "tsc --noEmit",
    "perf:analyze": "npm run build:analyze && npx webpack-bundle-analyzer .next/analyze/client.js",
    "perf:lighthouse": "lighthouse http://localhost:3000 --view",
    "perf:profile": "npm run build:profile && echo 'Check .next/profile-events.json for React profiling data'",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules",
    "preinstall": "npx only-allow npm"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@reduxjs/toolkit": "^2.8.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "set-cookie-parser": "^2.7.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@next/bundle-analyzer": "^15.3.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@nx/react": "^17.2.8",
    "babel-jest": "^29.7.0",
    "lighthouse": "^12.0.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
