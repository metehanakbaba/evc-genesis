  "projectType": "application",
  "sourceRoot": "packages/app/mobile",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start",
        "cwd": "packages/app/mobile"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start",
        "cwd": "packages/app/mobile"
      }
    },
    "android": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start --android",
        "cwd": "packages/app/mobile"
      }
    },
    "ios": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start --ios",
        "cwd": "packages/app/mobile"
      }
    },
    "web": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start --web",
        "cwd": "packages/app/mobile"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo build",
        "cwd": "packages/app/mobile"
      }
    }
  },
  "tags": ["type:app", "platform:mobile"]
